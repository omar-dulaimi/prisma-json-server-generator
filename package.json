{
  "name": "prisma-json-server-generator",
  "version": "0.0.0-rc.6",
  "description": "Prisma 2+ generator to emit a JSON file that can be run with json-server",
  "repository": "https://github.com/omar-dulaimi/prisma-json-server-generator",
  "bin": {
    "prisma-json-server-generator": "lib/generator.js"
  },
  "scripts": {
    "start": "tsc && cp -r src/lowdb lib && npx prisma generate",
    "package:publish": "./package.sh && cd package && npm publish"
  },
  "author": {
    "name": "Omar Dulaimi",
    "url": "https://github.com/omar-dulaimi"
  },
  "keywords": [
    "prisma",
    "prisma-client",
    "prisma-schema",
    "prisma-generator",
    "json-server",
    "prisma-json-server-generator"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@prisma/client": "~3.13.0",
    "prisma": "~3.13.0",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "@prisma/generator-helper": "^3.13.0",
    "@prisma/sdk": "^3.13.0",
    "zod": "^3.16.0",
    "@faker-js/faker": "^6.3.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "prisma": "^3.13.0",
    "typescript": "^4.6.4"
  },
  "bugs": {
    "url": "https://github.com/omar-dulaimi/prisma-json-server-generator/issues"
  },
  "homepage": "https://github.com/omar-dulaimi/prisma-json-server-generator#readme",
  "private": true
}
